{
    "name": "olyckne/pugs",
    "description": "The website for http://pugs.olyck.net",
    "license": "MIT",
    "type": "website",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "olyckne/pug": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "behat/behat": "^3.0",
        "behat/mink": "^1.6",
        "behat/mink-extension": "^2.0",
        "behat/mink-goutte-driver": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
